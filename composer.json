{
    "name": "phpfacile/openstreetmap",
    "description": "This service basically retrieves data from the Openstreetmap (OSM) data thanks to the Overpass API.",
    "keywords": ["openstreetmap", "overpass"],
    "homepage": "https://github.com/phpfacile/openstreetmap",
    "license": "MIT",
    "authors": [
        {
            "name": "PHP Facile!",
            "homepage": "https://www.phpfacile.com/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "squizlabs/php_codesniffer": "3.*"
    },
    "scripts": {
        "cs-check": "phpcs -s src",
        "cs-check-psr": "phpcs -s --standard=PSR1,PSR2 src",
        "cs-check-pear": "phpcs -s --standard=PEAR src",
        "cs-fix": "phpcbf src"
    },
    "autoload": {
        "psr-4": {
          "PHPFacile\\Openstreetmap\\": "src/"
        }
    }
}
